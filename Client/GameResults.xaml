<Window x:Class="Client.GameResults"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Client"
        mc:Ignorable="d"
        Title="GameResults" Height="450" Width="800"
        Background="AliceBlue">
    <Grid>
        <TextBlock x:Name="name" FontSize="16" VerticalAlignment="Top" HorizontalAlignment="Right" Margin="0,20,660,0" Width="100"/>
        <StackPanel Margin="0,60,0,0">
            <TextBlock FontSize="30" Width="199">Game Results:</TextBlock>
            <ListBox x:Name="Players" Height="244" Width="691"></ListBox>
            <StackPanel Orientation="Horizontal" Margin="20" Height="32" Width="244">
                <Button Click="backRoom_click">back to room</Button>
                <Button Margin="90,0,0,0" Click="backMenu_click">back to menu</Button>
            </StackPanel>
        </StackPanel>

    </Grid>
</Window>
