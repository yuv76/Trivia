<Window x:Class="Client.Room"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Client"
        mc:Ignorable="d"
        Title="Room" Height="450" Width="800"
        Background="AliceBlue">
    <Grid>
        <StackPanel Margin="90,90,103,36">
            <TextBlock x:Name="YOU_ARE_IN_ROOM_XXX" FontSize="20" Height="111"></TextBlock>
            <StackPanel Orientation="Horizontal">
                <TextBlock x:Name="OWNER" FontSize="16"></TextBlock>
                <ListBox x:Name="PLAYERS" Height="100" Width="338" Margin="240,0,0,0"></ListBox>
            </StackPanel>
            <Button Height="48" Width="122" Click="back_click" Margin="0,20,0,0">back</Button>
        </StackPanel>
    </Grid>
</Window>
